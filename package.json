    "node-machine-id": "^1.1.10",
    "node-unzip-2": "^0.2.7",
    "particles.js": "^2.0.0",
    "ramda": "^0.24.1",
    "resize-img": "^1.1.2",
    "sql.js": "^0.5.0",
    "three": "^0.93.0",
    "uuid": "^3.3.2",
    "vue": "^2.5.15",
    "vue-aplayer": "^1.5.0",
    "vue-electron": "^1.0.6",
    "vue-events": "^3.0.1",
    "vue-i18n": "^7.6.0",
    "vue-lever": "^0.1.2",
    "vue-router": "^2.7.0",
    "vuedraggable": "^2.16.0",
    "vuex": "^2.4.0",
    "zip-folder": "^1.0.0"
  },
